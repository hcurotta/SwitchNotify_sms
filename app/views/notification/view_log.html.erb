<h1>Activity Log</h1>

<p id="notice"><%= notice %></p>

<table>
	<tr>
		<th>From</th>
		<th>To</th>
		<th>Message</th>
		<th>Received At</th>
		<th>Device</th>
		<th>Status</th>
	</tr>
	<% @logs.each do |log| %>
	<tr>
		<td><%= log.from %></td>
		<td><%= log.to %></td>
		<td><%= log.message %></td>
		<td><%= log.created_at %></td>
		<td><%= log.notifier.device_id %></td>
		<td><%= log.notify_result %></td>
	</tr>
	<% end %>
</table>